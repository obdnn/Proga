#include <iostream>
using namespace std;

// Функція для обчислення числа Фібоначчі
int fibonacci(int n) {
    if (n <= 1)
        return n;
    return fibonacci(n - 1) + fibonacci(n - 2);
}

int main() {
    int n;

    cout << "Введіть номер числа Фібоначчі: ";
    cin >> n;

    cout << "Число Фібоначчі номер " << n << " дорівнює " << fibonacci(n) << endl;

    return 0;
}
